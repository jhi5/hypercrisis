//font declarations

@font-face
    font-family 'adventure'
    src url('adventure-webfont.woff2') format('woff2'),
        url('adventure-webfont.woff') format('woff'),
        url('adventure.ttf') format('truetype')

@font-face
    font-family 'okuda'
    src url('okuda-webfont.woff2') format('woff2'),
        url('okuda-webfont.woff') format('woff'),
        url('okuda.ttf') format('truetype')

//universal variables

$primary-font = 'okuda', helvetica
$secondary-font = 'adventure', georgia
$primary-spacing = letter-spacing .3rem

$l-border = 2rem solid black
$m-border =  1rem solid black 
$s-border = .5rem solid black
$s-border-w = .1rem solid white

$heading-ts = 7rem
$s-heading-ts = 2rem

$l-label-ts = 5rem
$label-ts = 3rem

$plain-ts = 1.25rem

$manual-color = #cc6699

//character-specific variables

$de-color = #333366
$gg-color = #8952a1
$kx-color = #663333
$ob-color = #cc6699
$qu-color = #336633
$rb-color = #666666
$sa-color = #009933

body
